node ('main'){
  properties([
    parameters([
      choice(
      choices:['one','two','three'],
      name: 'CHOUCE_NUMBERS'
      ),  
      booleanParam(
        defaultValue:true,
        description:"this is my name",
        name:'Boolean'
        ),
        text(
        defaultValue:''' defaultvalue for multiline ''',
        name:'Multiline_String'
        ),
        stage('stage1'){
        println("multi numbers"+CHOUCE_NUMBERS)
        println("multi string  ${Multiline_String}") 
    
    }
    ]
    

