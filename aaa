node ('main'){
  properties([
    parameters([
      choice(
      choices:['one','two','three'],
      name: 'CHOUCE_NUMBERS'
      ),  
      booleanParam(
        defaultValue:true,
        description:"this is my name",
        name:'Boolean'
        ),
        text(
        defaultValue:''' defaultvalue for multiline ''',
        name:'Multiline_String'
        ),
     stage('Stage 2'){
    println('Hello CHOCIE-Param!! ' + CHOUCE_NUMBERS)
    println("Hello BOOLEAN!! $Boolean")
    println("Hello MULTI-LINE-STRING!! ${Multiline_String}")
    println('Hello STRING-PARAMETER!! ' + STRING_PARAMETER)
    
  }
}
