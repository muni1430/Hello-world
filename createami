node('main'){
    def jsonitem
    stage('AWS CLI Version'){
        sh "aws --version"
    }
    stage('create an ami'){
        def cmd = 'aws ec2 create-image --instance-id i-053dee66115eb5539 --name "myjenkinsami" --description "amiserver" --region us-east-3'      
	def output = sh(script: cmd)
        jsonitem = readJSON text: output
        println(jsonitem)
    }
}

