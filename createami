node('main'){
    def jsonitem
    stage('AWS CLI Version'){
        sh "aws --version"
    }
    stage('create an ami'){
        //sts_client = boto3.client('sts', region_name='us-east-1c', endpoint_url='https://sts.us-east-1c.amazonaws.com')
        def cmd = 'aws ec2 create-image --instance-id i-086045db590bf98f8 --name "jenserve" --description "amiserver" --region_name us-east-1' 
	def output = sh(script: cmd)
        jsonitem = readJSON text: output
        println(jsonitem)
    }
}

